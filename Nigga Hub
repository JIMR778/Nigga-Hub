local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Negger hub", "Sentinel")



-- Main
local Main = Window:NewTab("Main")
local MainSection = Main:NewSection("Main")

-- Aimlock
MainSection:NewButton("AimLock", "You get AimLock", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/JIMR778/Nigga-Hub/refs/heads/main/Aimbot"))()
end)

-- Esp
